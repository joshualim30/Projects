{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "extensions": {
    "firestore-send-email": "firebase/firestore-send-email@0.2.4",
    "firestore-stripe-payments": "stripe/firestore-stripe-payments@0.3.4"
  }
}
